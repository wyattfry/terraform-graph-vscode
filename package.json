{
  "name": "terraform-graph-vscode",
  "displayName": "Terraform Graph Viewer",
  "description": "Preview of the Terraform dependency graph, but pretty",
  "version": "0.0.1",
  "publisher": "vscode",
  "private": true,
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other",
    "Visualization"
  ],
  "activationEvents": [
    "onCommand:terraform-graph-viewer.open"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "terraform-graph-viewer.open",
        "title": "Terraform: Open Dependency Graph"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/cytoscape": "^3.19.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  },
  "dependencies": {
    "cytoscape": "^3.21.1",
    "cytoscape-cola": "^2.5.1"
  }
}